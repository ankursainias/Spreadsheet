	<h1>Users</h1>
	<table>
	  <tr>
	    
	    <th>First Name</th>
	    <th>Last Name</th>
	    <th>Email ID</th>
	  </tr>
	  <% @users.each_with_index do |user,index |%>
	    <td><%= user.first_name %></td>
	    <td><%= user.last_name %></td>
	    <td><%= user.email_id %></td>
	  </tr>
	  <%end%>

	</table>